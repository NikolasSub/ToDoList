<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefHeight="380.0" prefWidth="709.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PrimaryController">
   <children>
      <Label prefHeight="43.0" prefWidth="115.0" text="ToDoList" textAlignment="CENTER">
         <font>
            <Font size="29.0" />
         </font>
      </Label>
      <TableView fx:id="tableView" prefHeight="263.0" prefWidth="419.0">
        <columns>
          <TableColumn fx:id="tableColumnDate" prefWidth="342.0" text="Date" />
          <TableColumn fx:id="tableColumnAction" prefWidth="391.0" text="Action" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="button" mnemonicParsing="false" onAction="#runSecondaryStage" prefHeight="35.0" prefWidth="111.0" text="Add" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="5.0" />
   </padding>
</VBox>
